{
  "name": "banco-chain-sdk",
  "version": "1.1.0",
  "description": "Banco Chain SDK for Node.js",
  "main": "lib/banco-chain.js",
  "scripts": {
    "build": "npm run tsc",
    "tsc": "tsc -p ./tsconfig.json",
    "tsc:watch": "tsc -w",
    "lint": "tslint -p ./tsconfig.json --fix",
    "lint:no-fix": "tslint -p ./tsconfig.json",
    "test": "npm run build && mocha",
    "ci": "npm run tsc && npm run lint:no-fix && nyc mocha -t 6000",
    "prepublishOnly": "npm run tsc && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RABC-Group/banco-chain-sdk-nodejs.git"
  },
  "keywords": [
    "banco",
    "chain",
    "rabc",
    "banco chain",
    "rabc group",
    "sdk"
  ],
  "author": "RABC Group <it@rabc.sg>",
  "license": "ISC",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "bugs": {
    "url": "https://github.com/RABC-Group/banco-chain-sdk-nodejs/issues"
  },
  "homepage": "https://github.com/RABC-Group/banco-chain-sdk-nodejs#readme",
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "include": [
      "lib"
    ],
    "extension": [
      ".ts"
    ],
    "check-coverage": true,
    "reporter": [
      "text-summary",
      "json",
      "html"
    ],
    "sourceMap": true
  },
  "ci": {
    "version": "12, 14, 16"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/is": "^0.0.21",
    "@types/jsonwebtoken": "^8.5.8",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "should": "^13.2.3",
    "sinon": "^14.0.0",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "camelcase-keys": "^4.2.0",
    "is": "^3.3.0",
    "jsonwebtoken": "^8.5.1",
    "urllib": "^2.38.0"
  }
}
